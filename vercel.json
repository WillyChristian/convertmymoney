{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/convert",
      "dest": "/app.js",
      "methods": ["GET"]
    }
  ]
}
